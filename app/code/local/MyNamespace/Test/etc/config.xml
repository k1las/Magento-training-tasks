    <frontend>
        <layout>
            <updates>
                <mynamespace_test>
                    <file>mynamespace_test/local.xml</file>
                </mynamespace_test>
            </updates>
        </layout>
        <routers>
            <mynamespace_test>
                <use>standard</use>
                <args>
                    <module>MyNamespace_Test</module>
                    <frontName>test</frontName>
                </args>
            </mynamespace_test>
        </routers>

    </frontend>
</config>
