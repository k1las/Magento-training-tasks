<?xml version="1.0" ?>
<config>
    <modules>
        <MyNamespace_Test>
            <version>0.0.1</version>
        </MyNamespace_Test>
    </modules>

    <global>
        <models>
            <mynamespace_test>
                <class>MyNamespace_Test_Model</class>
            </mynamespace_test>
            <test>
                <class>MyNamespace_Test_Model</class>
                <resourceModel>test_resource</resourceModel>
            </test>
            <test_resource>
                <class>MyNamespace_Test_Model_Resource</class>
                <entities>
                    <test>
                        <table>test</table>
                    </test>
                </entities>
            </test_resource>
        </models>
        <events>
            <customer_login>
                <observers>
                    <mynamespace_test_model_observer>
                        <type>singleton</type>
                        <class>mynamespace_test/observer</class>
                        <method>customerLogin</method>
                    </mynamespace_test_model_observer>
                </observers>
            </customer_login>
            <customer_logout>
                <observers>
                    <mynamespace_test_model_observer>
                        <type>singleton</type>
                        <class>mynamespace_test/observer</class>
                        <method>customerLogout</method>
                    </mynamespace_test_model_observer>
                </observers>
            </customer_logout>
        </events>
        <blocks />
        <extraconfig />
        <helpers>
            <test>
                <class>MyNamespace_Test_Helper</class>
            </test>
        </helpers>
        <resources>
            <mynamespacetest_setup>
                <setup>
                    <module>MyNamespace_Test</module>
                </setup>
            </mynamespacetest_setup>
        </resources>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <MyNamespace_Test before="Mage_Adminhtml">MyNamespace_Test_Adminhtml</MyNamespace_Test>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
